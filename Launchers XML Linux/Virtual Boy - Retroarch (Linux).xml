<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <name>Virtual Boy (Retroarch)</name>
  <category>Nintendo</category>
  <Launcher_NFO>../Launcher assets Nintendo/Virtual Boy.nfo</Launcher_NFO>
  <platform>Nintendo Virtual Boy</platform>
  <application>/usr/bin/retroarch</application>
  <args>-L /usr/lib/x86_64-linux-gnu/libretro/mednafen_vb_libretro.so -f -v &quot;$rom$&quot;</args>
  <ROM_path>/media/storage/Roms/nintendo-vb/</ROM_path>
  <ROM_ext>zip</ROM_ext>
  <ROM_asset_path>/media/storage/Assets/nintendo-vb/</ROM_asset_path>
  <Asset_Prefix>../Launcher assets Nintendo/Virtual Boy</Asset_Prefix>
</launcher>
</advanced_emulator_launcher_configuration>
