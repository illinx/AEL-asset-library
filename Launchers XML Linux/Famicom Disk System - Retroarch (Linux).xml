  <ROM_path>/media/storage/Roms/NES/</ROM_path>
  <ROM_ext>zip|fds</ROM_ext>
  <ROM_asset_path>/media/storage/Assets/FDS/</ROM_asset_path>
  <Asset_Prefix>../Launcher assets Nintendo/Famicom Disk System</Asset_Prefix>
</launcher>
</advanced_emulator_launcher_configuration>

