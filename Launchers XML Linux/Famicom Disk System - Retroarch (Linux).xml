<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <name>Famicom Disk System (Retroarch)</name>
  <category>Nintendo</category>
  <Launcher_NFO>../Launcher assets Nintendo/Famicom Disk System.nfo</Launcher_NFO>
  <platform>Nintendo Famicom Disk System</platform>
  <application>/usr/bin/retroarch</application>
