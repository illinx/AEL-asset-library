<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <name>Sinclair ZX Spectrum (Retroarch)</name>
  <category>Computers</category>
  <Launcher_NFO>../Launcher assets Computers/Sinclair ZX Spectrum.nfo</Launcher_NFO>
  <platform>Sinclair ZX Spectrum</platform>
  <application>/usr/bin/retroarch</application>
  <args>-L /usr/lib/x86_64-linux-gnu/libretro/fuse_libretro.so -f -v &quot;$rom$&quot;</args>
  <ROM_path>/media/storage/Roms/com-spectrum/</ROM_path>
  <ROM_ext>zip</ROM_ext>
  <ROM_asset_path>/media/storage/Assets/com-spectrum/</ROM_asset_path>
  <Asset_Prefix>../Launcher assets Computers/Sinclair ZX Spectrum</Asset_Prefix>
</launcher>
</advanced_emulator_launcher_configuration>
