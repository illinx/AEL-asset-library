<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!-- Standalone launcher for Doom using Retroarch -->
<advanced_emulator_launcher_configuration>
<launcher>
  <name>Doom (Retroarch)</name>
  <category>Games</category>
  <Launcher_NFO>../Launcher assets Games/Doom.nfo</Launcher_NFO>
  <platform>Unknown</platform>
  <application>/usr/bin/retroarch</application>
  <args>-L /usr/lib/x86_64-linux-gnu/libretro/prboom_libretro.so -v -f /home/kodi/Games/doom/doom.wad</args>
  <Asset_Prefix>../Launcher assets Games/Doom</Asset_Prefix>
</launcher>
</advanced_emulator_launcher_configuration>
