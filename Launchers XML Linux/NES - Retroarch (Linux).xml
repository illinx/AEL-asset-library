<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <name>NES (Retroarch)</name>
  <category>Nintendo</category>
  <Launcher_NFO>../Launcher assets Nintendo/NES.nfo</Launcher_NFO>
  <platform>Nintendo NES</platform>
  <application>/usr/bin/retroarch</application>
  <args>-L /home/kodi/bin/libretro/fceumm_libretro.so -f -v &quot;$rom$&quot;</args>
  <args_extra>-L /home/kodi/bin/libretro/nestopia_libretro.so -f -v &quot;$rom$&quot;</args_extra>
  <ROM_path>/media/storage/Roms/NES/</ROM_path>
  <ROM_ext>zip</ROM_ext>
  <ROM_asset_path>/media/storage/Assets/NES/</ROM_asset_path>
  <Asset_Prefix>../Launcher assets Nintendo/NES</Asset_Prefix>
</launcher>
</advanced_emulator_launcher_configuration>
